{
  "name": "@example-org/common",
  "version": "1.0.0",
  "description": "Shared utilities, types, middleware, config, for microservices",
  "private": true,
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./openapi/*": {
      "types": "./src/openapi/*.d.ts",
      "default": "./src/openapi/*.js"
    },
    "./utils/*": {
      "types": "./src/utils/*.d.ts",
      "default": "./src/utils/*.js"
    },
    "./types/*": {
      "types": "./src/types/*.d.ts",
      "default": "./src/types/*.js"
    },
    "./health/*": {
      "types": "./src/health/*.d.ts",
      "default": "./src/health/*.js"
    },
    "./middleware/*": {
      "types": "./src/middleware/*.d.ts",
      "default": "./src/middleware/*.js"
    },
    "./db-utils/*": {
      "types": "./src/db-utils/*.d.ts",
      "default": "./src/db-utils/*.js"
    },
    "./logging/*": {
      "types": "./src/logging/*.d.ts",
      "default": "./src/logging/*.js"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "7.3.4",
    "dotenv": "^16.6.1",
    "express": "5.1.0",
    "express-rate-limit": "8.0.1",
    "http-status-codes": "2.3.0",
    "mongoose": "^9.2.1",
    "pino": "9.9.0",
    "pino-http": "10.5.0",
    "pino-pretty": "13.1.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@example-org/vite-config": "workspace:*",
    "@types/express": "5.0.3",
    "@types/pino": "^7.0.5",
    "@types/supertest": "6.0.3",
    "@types/swagger-ui-express": "4.1.8",
    "rimraf": "^6.1.2",
    "supertest": "7.1.4",
    "swagger-ui-express": "5.0.1",
    "tsup": "8.5.0",
    "typescript": "^5.5.4",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
